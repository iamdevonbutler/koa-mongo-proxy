{
  "name": "mongorules",
  "version": "0.2.0-alpha",
  "description": "A small but fierce wrapper around the native mongodb driver leveraging ES6 proxy black magic and schemas.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/helpers/setup.js ./tests/**/*.test.js",
    "debug": "NODE_ENV=test DEBUG=mongorules ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/helpers/setup.coffee ./tests/**/*.test.coffee",
    "test:preprocess": "NODE_ENV=test DEBUG=mongorules ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/helpers/setup.js ./tests/unit/preprocess.test.js ./tests/integration/*.preprocess.test.js",
    "test:preprocess:unit": "NODE_ENV=test DEBUG=mongorules ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/unit/preprocess.test.js",
    "test:preprocess:integration": "NODE_ENV=test DEBUG=mongorules ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/helpers/setup.js ./tests/integration/*.preprocess.test.js",
    "test:utils:unit": "NODE_ENV=test DEBUG=mongorules ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/unit/utils.test.js",
    "test:schema:unit": "NODE_ENV=test DEBUG=mongorules ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/unit/schema.test.js",
    "test:transform:unit": "NODE_ENV=test DEBUG=mongorules ./node_modules/.bin/mocha --reporter spec --ui bdd --bail --colors ./tests/unit/transform.test.js"
  },
  "engines": {
    "node": ">=6.0.x"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "node",
    "wrapper",
    "abstraction",
    "schema",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamdevonbutler/node-mongorules.git"
  },
  "author": "Jay Pescione <jpescione@gmail.com> (https://github.com/iamdevonbutler)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamdevonbutler/node-mongorules/issues"
  },
  "homepage": "https://github.com/iamdevonbutler/node-mongorules#readme",
  "peerDependencies": {
    "mongodb": ">=2.0.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "mocha": "^2.3.0",
    "mongodb": "^2.0.0"
  },
  "dependencies": {
    "co": "4.6.0",
    "debug": "2.6.0",
    "lodash": "4.17.4",
    "lodash-deep": "1.6.0",
    "moment": "2.17.1",
    "xss-filters": "1.2.7"
  }
}
